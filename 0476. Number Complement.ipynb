   "source": [
    "def findComplement(num):\n",
    "    \"\"\"\n",
    "    :type num: int\n",
    "    :rtype: int\n",
    "    \"\"\"\n",
    "\n",
    "    i = 1\n",
    "    while i <= num:\n",
    "        i = i << 1\n",
    "    return (i-1) ^ num"
   ]
  },
